<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor name="basic">
    <fileSets>
        <fileSet filtered="true" packaged="true">
            <directory>src/main/java</directory>
            <includes>
                <include>**/*.java</include>
            </includes>
        </fileSet>
        <fileSet filtered="false" packaged="false">
            <directory>src/main/native</directory>
            <includes>
                <include>*.c</include>
                <include>*.cc</include>
                <include>*.c++</include>
                <include>*.C</include>
                <include>*.cpp</include>
                <include>*.cxx</include>
                <include>*.h</include>
                <include>*.h++</include>
                <include>*.hpp</include>
                <include>*.hxx</include>
            </includes>
        </fileSet>
        <fileSet filtered="false" packaged="false">
            <directory>src/main/autotools</directory>
            <includes>
                <include>*.m4</include>
                <include>AUTHORS</include>
                <include>ChangeLog</include>
                <include>configure.ac</include>
                <include>Makefile.am</include>
                <include>NEWS</include>
                <include>README</include>
            </includes>
        </fileSet>
    </fileSets>
</archetype-descriptor>
