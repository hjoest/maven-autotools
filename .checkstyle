<?xml version="1.0" encoding="UTF-8"?>
<fileset-config file-format-version="1.2.0" simple-config="true">
    <local-check-config name="Maven-Autotools" location="src/checkstyle/checkstyle.xml" type="project" description="">
        <additional-data name="protect-config-file" value="false"/>
    </local-check-config>
    <fileset name="All" enabled="true" check-config-name="Maven-Autotools" local="true">
        <file-match-pattern match-pattern="." include-pattern="true"/>
    </fileset>
    <filter name="FilesFromPackage" enabled="true">
        <filter-data value="maven-autotools-plugin/src/test/java"/>
    </filter>
    <filter name="NonSrcDirs" enabled="true"/>
</fileset-config>
